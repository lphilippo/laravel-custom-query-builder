{
    "name": "lgrevelink/laravel-custom-query-builder",
    "description": "Utility project for creating custom query builders in Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Lars Grevelink"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": ">=11.0",
        "illuminate/support": ">=11.0"
    },
    "require-dev": {
        "laravel/framework": ">=11.0",
         "phpunit/phpunit": ">=11.0"
    },
    "autoload": {
        "psr-4": {
            "LGrevelink\\CustomQueryBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "LGrevelink\\CustomQueryBuilder\\ServiceProvider"
            ]
        }
    }
}
